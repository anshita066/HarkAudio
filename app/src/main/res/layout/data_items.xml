<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>
        <variable
            name="question"
            type="com.harkaudio.fragments.search.harklist.harklist_dataclasses.Question" />
    </data>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootLayout"
    android:backgroundTint="@color/white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="14dp"
    android:layout_marginTop="8dp"
    android:layout_marginEnd="18dp"
    android:layout_marginBottom="8dp"
    android:background="@drawable/round_corners"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:context="com.harkaudio.fragments.search.SearchFragment">


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv1"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="257dp"
        android:layout_marginBottom="140dp"
        android:adjustViewBounds="true"
        android:elevation="10dp"
        android:visibility="gone"
        android:background="@drawable/image_corners"
        app:shapeAppearanceOverlay="@style/rounded"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv2"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="140dp"
        android:elevation="8dp"
        android:visibility="gone"
        app:shapeAppearanceOverlay="@style/rounded"
        app:layout_constraintLeft_toLeftOf="@id/iv1"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv3"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="140dp"
        android:adjustViewBounds="true"
        android:elevation="6dp"
        android:visibility="gone"
        app:shapeAppearanceOverlay="@style/rounded"
        app:layout_constraintLeft_toLeftOf="@id/iv2"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv4"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="158dp"
        android:layout_marginBottom="140dp"
        android:elevation="4dp"
        android:visibility="gone"
        app:shapeAppearanceOverlay="@style/rounded"
        app:layout_constraintLeft_toLeftOf="@id/iv3"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv5"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="140dp"
        android:elevation="2dp"
        android:visibility="gone"
        app:shapeAppearanceOverlay="@style/rounded"
        app:layout_constraintLeft_toLeftOf="@id/iv4"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:textSize="24dp"
        style="@style/GangsterRegular"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv1" />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv6"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="59dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv1"
        app:layout_constraintVertical_bias="0.151"
        app:shapeAppearanceOverlay="@style/circle" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="17dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintLeft_toRightOf="@+id/iv6"
        app:layout_constraintTop_toBottomOf="@id/tv1" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="58dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintTop_toBottomOf="@id/tv2"
        app:layout_constraintLeft_toRightOf="@id/iv6" />


    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:layout_marginTop="23dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintRight_toLeftOf="@id/clips"
        app:layout_constraintTop_toBottomOf="@id/tv1" />

    <TextView
        android:id="@+id/clips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="20dp"
        android:text="CLIPS"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv1" />


    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="59dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintTop_toBottomOf="@id/tv4"
        app:layout_constraintRight_toLeftOf="@id/hr"/>

    <TextView
        android:id="@+id/hr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginStart="2dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintTop_toBottomOf="@id/clips"
        app:layout_constraintLeft_toRightOf="@id/tv5" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="59dp"
        android:layout_marginStart="4dp"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintTop_toBottomOf="@id/tv4"
        app:layout_constraintStart_toEndOf="@id/hr" />

    <TextView
        android:id="@+id/mins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="59dp"
        android:text="MINS"
        android:textSize="11dp"
        style="@style/SFMono"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/clips"
        app:layout_constraintLeft_toRightOf="@id/tv6"

       />



    <ImageView
        android:id="@+id/play"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="291dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/ic_play_button"
        app:layout_constraintTop_toBottomOf="@id/mins"
        app:layout_constraintRight_toRightOf="parent"
         />

    <ImageView
        android:id="@+id/download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/ic_download"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/bookmark"
        app:layout_constraintTop_toBottomOf="@+id/iv6"
         />

    <ImageView
        android:id="@+id/bookmark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="254dp"
        android:layout_marginBottom="21dp"
        android:src="@drawable/ic_bookmark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/download"
        app:layout_constraintTop_toBottomOf="@id/tv3" />

    <ImageView
        android:id="@+id/dotmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="30dp"
        android:src="@drawable/ic_dotmenu"
        app:layout_constraintTop_toBottomOf="@id/tv3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/bookmark" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>